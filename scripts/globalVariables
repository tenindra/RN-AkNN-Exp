COL_index_parameters="gtree_fanout=4,gtree_maxleafsize=128,road_fanout=4,road_levels=9,silc_maxquadtreeleafsize=1000,tnr_gridsize=128,alt_numlandmarks=16,alt_landmarktype=random"
