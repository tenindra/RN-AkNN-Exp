CAL_index_parameters="gtree_fanout=4,gtree_maxleafsize=256,road_fanout=4,road_levels=10,silc_maxquadtreeleafsize=1000,tnr_gridsize=128,alt_numlandmarks=16,alt_landmarktype=random"
